 "html": "{% if doc.custom_certificate %}\n  <img src=\"{{ doc.custom_certificate }}\" style=\"width:100vw; height:100vh; object-fit:cover;\" alt=\"Full Page Image\">\n{% else %}\n<div style=\"position: relative; min-height: 100vh; font-family: Arial, sans-serif;\">\n\n  <!-- Logo -->\n  <div style=\"text-align:left; margin-bottom:20px;\">\n    <img src=\"/files/Screenshot from 2025-07-15 08-45-36.png\" width=\"80\" height=\"80\" alt=\"Logo\">\n  </div>\n\n  <!-- Header -->\n  <div style=\"text-align:center; margin-bottom:20px;\">\n    <h1 style=\"color:blue; margin:5px 0; font-size:30px;\">NUMERO UNO TRAINING &amp; CONSULTING LLC</h1>\n    {% set course = frappe.get_doc(\"Course\", doc.course) %}\n    <h3 style=\"margin:0;\">CERTIFICATE OF {{ course.custom_certificate_type }}</h3>\n  </div>\n\n  <!-- Body -->\n  <div style=\"text-align:center; margin:20px 0 140px;\"> <!-- leave bottom gap for footer -->\n    <h4><strong>This is to certify that</strong></h4>\n    <h2 style=\"margin:0 0 20px;\">{{ doc.student_name }}</h2>\n\n    {% if doc.course %}\n    <strong>\n      <p>{{ doc.course }}</p></strong>\n    {% endif %}\n\n    <p>{{ course.description or \"\" }}</p>\n\n    {% set dt = frappe.utils.get_datetime(doc.custom_certificate_validity_date) %}\n    {% if dt %}\n      {% set day = dt.day %}\n      {% set suffix = (\n          'th' if 11 <= day <= 13 else\n          'st' if day % 10 == 1 else\n          'nd' if day % 10 == 2 else\n          'rd' if day % 10 == 3 else\n          'th'\n      ) %}\n      <p style=\"margin-top:10px; font-style:italic;\">\n        Awarded on {{ day }}{{ suffix }} {{ dt.strftime(\"%B %Y\") }}\n      </p>\n    {% endif %}\n\n    <!-- Course details -->\n    <table style=\"width:60%; margin:20px auto 0; font-size:12px;\">\n      <tr>\n        <td><strong>Course Date</strong></td>\n        <td>:</td>\n        <td style=\"border-bottom:1px dotted #000;\">\n          {% if doc.course_start_date != doc.course_end_date %}\n            {{ frappe.utils.get_datetime(doc.course_start_date).strftime(\"%d-%m-%Y\") }}\n            to\n            {{ frappe.utils.get_datetime(doc.course_end_date).strftime(\"%d-%m-%Y\") }}\n          {% else %}\n            {{ frappe.utils.get_datetime(doc.course_start_date).strftime(\"%d-%m-%Y\") }}\n          {% endif %}\n        </td>\n      </tr>\n      <tr>\n        <td><strong>Expiry Date</strong></td>\n        <td>:</td>\n        <td style=\"border-bottom:1px dotted #000;\">\n          {% if doc.validity_period and doc.validity_period != 'NA' %}\n            {{ frappe.utils.get_datetime(doc.certificate_validity_date).strftime(\"%d-%m-%Y\") }}\n          {% else %}\n            N/A\n          {% endif %}\n        </td>\n      </tr>\n      <tr>\n        <td><strong>Certificate No</strong></td>\n        <td>:</td>\n        <td style=\"border-bottom:1px dotted #000;\">{{ doc.name }}</td>\n      </tr>\n    </table>\n  </div>\n\n  <!-- Fixed Bottom Section -->\n  <div style=\"position:absolute; bottom:0; left:0; width:100%;\">\n    <table style=\"width:100%; margin-bottom:10px; border-collapse:collapse;\">\n      <tr>\n        <!-- Instructor -->\n        <td style=\"width:33%; text-align:center;\">\n          {% set instructor_name = frappe.db.get_value(\"Student Group Instructor\", {\"parent\": doc.student_group}, \"instructor_name\") %}\n          {% if instructor_name %}\n            <img src=\"/files/Screenshot from 2025-07-15 08-49-17.png\" width=\"120\" height=\"80\" alt=\"Instructor Signature\" style=\"display:block; margin:0 auto 8px;\">\n            {{ instructor_name }}\n          {% endif %}\n          <br>Instructor\n        </td>\n\n        <!-- QR -->\n        <td style=\"width:33%; text-align:center;\">\n          {% set base = frappe.utils.get_url() %}\n          {% set print_url = base ~ \"/certificate-verification?cert=\" ~ doc.name|urlencode %}\n          <img src=\"https://barcode.tec-it.com/barcode.ashx?data={{ print_url|urlencode }}&code=QRCode&dpi=150\"\n               width=\"120\" height=\"120\" alt=\"QR code for {{ doc.name }}\">\n        </td>\n\n        <!-- Director -->\n        <td style=\"width:33%; text-align:center;\">\n          <img src=\"/files/2953d790-08e3-4bef-972c-fa557de8bc12.jpeg\" width=\"120\" height=\"80\"\n               alt=\"Director Signature\" style=\"display:block; margin:0 auto 8px;\">\n          Academic Director\n        </td>\n      </tr>\n    </table>\n\n    <!-- Footer Image -->\n    <div style=\"text-align:center;\">\n      <img src=\"/files/FOOTER.png\" style=\"width:100%;\" alt=\"Footer\">\n    </div>\n  </div>\n\n</div>\n{% endif %}\n",
